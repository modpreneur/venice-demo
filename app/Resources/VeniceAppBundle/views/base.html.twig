{% block bodyFull %}
    <body class="{% block bodyClass %}{% endblock %}">
    <input type="checkbox" id="buy-modal-input" class="display-none"/>
    <input type="checkbox" id="start-guilde" class="display-none"/>
    {% block guideModal %}{% endblock %}
    {% block buyModal %}{% endblock %}

    {% block videoModal %}
        <div>
            <div id="video-box-open" class="video-modal small-hidden">
                <div style="width: 100%;height: 100%;position: fixed;" onclick="closeVideo()"></div>
                <div class="modal-video-box">
                    <div id="modal-video-box">
                        <a id="close-video-button" onclick="closeVideo()" title="Close" class="close-video-modal cross"><i class="ff ff-cross2"></i></a>
                        <div id="video-div"></div>
                    </div>
                </div>
            </div>
        </div>
    {% endblock %}


    <div id="wrap-body" class="blured">
        {% set pageMenuHolder = knp_menu_get('VeniceFrontBundle:MenuBuilder:build') %}

        {% block body %}
            <div class="jumbotron jumbotron-{{ random(7) }} {% block jumbotronClass %}{% endblock %}">
                <div class="float-header-bg"></div>
                <div class="header-filter">
                    <div class="section main-header fix-header-only" id="main-header">
                        <div class="container">
                            {% block header %}
                                {% include "VeniceFrontBundle:Default:header.html.twig" %}
                            {% endblock %}
                        </div>
                    </div>

                    <div class="section main-header-profile">
                        <div class="container">
                            {% block headerInside %}
                                {% if app.user is not null %}
                                    {{ include("VeniceFrontBundle:Default:headerProfile.html.twig", {"user":app.user}, with_context=false) }}
                                {% endif %}
                            {% endblock %}
                        </div>
                    </div>

                    <div class="section main-menu">
                        <div class="container">
                            {% block menu %}
                                Menu here
                                {% include "VeniceFrontBundle:Default:menu.html.twig" %}
                            {% endblock %}
                        </div>
                    </div>
                </div>
            </div>

            {% block flashMessages %}
                <div id="flashMessages">
                    {% for type in ["danger","warning","success","info"] %}
                        {% for flashMessage in app.session.flashBag.get(type) %}
                            {{ include("VeniceFrontBundle:Default:flashMessage.html.twig", {"type":type,"id":loop.index,"text":flashMessage}, with_context=false) }}
                        {% endfor %}
                    {% endfor %}
                </div>
            {% endblock %}

            {% block contentFull %}
                <div class="section main-content">
                    <div class="container">

                        {% block content %}
                        {% endblock %}
                    </div>
                    {% block section %}
                    {% endblock %}
                </div>
            {% endblock %}

            <div class="section main-footer">
                <div class="container">
                    {% block footer %}
                        {% include "VeniceFrontBundle:Default:footer.html.twig" %}
                    {% endblock %}
                </div>
            </div>
        {% endblock %}


