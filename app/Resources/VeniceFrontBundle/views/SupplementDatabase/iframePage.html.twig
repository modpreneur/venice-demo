{% extends "VeniceAppBundle::base.html.twig" %}

{% block title %}
    {{ pageTitle }}
{% endblock %}

{% block jumbotronClass %}jumbotron-dashboard{% endblock %}

{% block content %}
    <div class="row">
        <div class="span-large-24">
            <div class="block-title">
                <div class="static-page">
                    <h1>{{ pageTitle }}</h1>
                    {% if iframeHeight is defined %}
                    <iframe style="border: 0px;" src="{{ iframeLink }}" height="{{ iframeHeight }}" width="100%"></iframe>
                    {% else %}
                    <iframe style="border: 0px;" src="{{ iframeLink }}" height="10000" width="100%"></iframe>
                    {% endif %}
                </div>
            </div>
        </div>
    </div>
{% endblock %}