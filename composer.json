                "url_flo_fit_youtube" : "SYMFONY__URL_FLO_FIT_YOUTUBE",
                "url_flo_rida_facebook" : "SYMFONY__URL_FLO_RIDA_FACEBOOK",
                "url_flo_rida_twitter" : "SYMFONY__URL_FLO_RIDA_TWITTER",
                "url_flo_rida_instagram" : "SYMFONY__URL_FLO_RIDA_INSTAGRAM",
