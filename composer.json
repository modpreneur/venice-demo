                "facebook_client_id" : "SYMFONY__FACEBOOK_CLIENT_ID",
                "facebook_client_secret" : "SYMFONY__FACEBOOK_CLIENT_SECRET",
                "twitter_client_id" : "SYMFONY__TWITTER_CLIENT_ID",
                "twitter_client_secret" : "SYMFONY__TWITTER_CLIENT_SECRET",
                "twitter_client_token" : "SYMFONY__TWITTER_CLIENT_TOKEN",
                "twitter_client_token_secret" : "SYMFONY__TWITTER_CLIENT_TOKEN_SECRET",
                "instagram_client_id" : "SYMFONY__INSTAGRAM_CLIENT_ID",
