                "number_of_blog_posts" : "SYMFONY__NUMBER_OF_BLOG_POSTS",
                "number_of_product_posts" : "SYMFONY__NUMBER_OF_PRODUCT_POSTS",
                "forum_url" : "SYMFONY__FORUM_URL",
