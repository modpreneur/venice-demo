{
  "name": "Venice",
  "version": "0.0.1",
  "description": "JS part of Venice",
  "author": "fiser@modpreneur.com",
  "prefix": "web/js",
  "contributors": [],
  "main": "web/js/app/main.js",
  "directories": {
    "lib": "./lib",
    "packages": "jspm_packages"
  },
  "keywords": [
    "modpreneur",
    "venice"
  ],
  "scripts": {
    "build-trinity": "jspm bundle trinity - lodash ./bundles/trinity.bundle.js --inject",
    "build-vendor": "jspm bundle lodash + jquery ./bundles/vendor.bundle.min.js --inject --minify",
    "controllers": "nodejs ./scripts/buildControllers.js",
    "build": "nodejs ./scripts/buildControllers.js && jspm bundle-sfx app/main.js ./venice.bundle.min.js --minify"
  },
  "dependencies": {},
  "devDependencies": {},
  "comment-devDependecies": "You need to have installed jspm globally - docker will install it automacally. It CANNOT be defined in devDependecies locally.",
  "private": true,
  "license": "MIT",
  "jspm": {
    "directories": {
      "lib": "./lib"
    },
    "dependencies": {
      "async": "npm:async@^1.5.0",
      "flux": "npm:flux@^2.1.1",
      "jquery": "github:components/jquery@^2.1.4",
      "lodash": "npm:lodash@^3.10.1",
      "mousetrap": "npm:mousetrap@^1.5.3",
      "trinity": "github:modpreneur/trinityJS@master"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
